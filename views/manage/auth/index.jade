extends ../../manage

block content
		.container
			.row 
			.row.h1.logo LOGIN
			p Please login below
			.row <br><br>
			.row
				form( method='post', action='/admin/login',enctype='multipart/form-data')
					.form-group
						label Name :
						input.form-control(type='text', name='username',placeholder='Username')
					.form-group
						label Password:
						input.form-control(type='password', name='password', placeholder='Password')
					input.btn.btn-primary(name='submit',type='submit',value='Login') 
